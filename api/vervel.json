{
    "builds": [
        {
            "src": "pip install -r requirements.txt",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        },
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "(.*)",
            "destination": "index.py"
        }
    ],
    "trailingSlash": true
}